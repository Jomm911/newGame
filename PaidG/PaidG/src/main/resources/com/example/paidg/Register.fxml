<AnchorPane prefHeight="498.0" prefWidth="479.0" style="-fx-background-color: #212328;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.paidg.RegisterController">
