      <TextField fx:id="firstNameField" layoutX="17.0" layoutY="70.0" prefHeight="42.0" prefWidth="218.0" promptText="First name" style="-fx-background-color: #32353C; -fx-background-radius: 10; -fx-text-fill: white;">
